from test.Step08_if import isMan

def test(*args, **kwargs):
    print args
    print kwargs
    print "--- test() ---"


if __name__ == '__main__':
    test(1)
    test(1,2)
    test("a","b","c")
    test(name="gura")
    test(isMan=True)
    test(name="monkey",isMan=False)

