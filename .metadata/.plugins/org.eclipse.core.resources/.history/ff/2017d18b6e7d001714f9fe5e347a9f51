#-*- coding: utf-8 -*-

class Car:
    #필드(속성, 저장소)
    name=u"소나타"
    
    #메소드(기능, 함수)
    def drive(self):
        print u"달려요"
        
        
    #메소드
    def showInfo(self):
        pass
        
if __name__ == '__main__':
    # Car 클래스를 이용해서 객체를 생성하고 참조값을 변수에 담기
    car1=Car()
    # Car 객체의 필드 참조해서 콘솔에 출력하기
    print "car1.name :", car1.name
    # Car 객체의 메소드 호출하기
    car1.drive()
    
    
