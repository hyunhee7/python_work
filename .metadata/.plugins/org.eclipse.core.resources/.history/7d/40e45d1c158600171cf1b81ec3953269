#-*- coding: utf-8 -*-

from django.shortcuts import render
from member.models import Member

# Create your views here.

def list(request):
    # Member 모델의 모든 데이터를 가지고 온다.
    member_list=Member.objects.all()
    return render(
        request,
        'member/list.html',
        {'member_list':member_list})
    
