#-*- coding: utf-8 -*-

class Car:
    #필드 정의하기
    company=None #제조사
    name=None #이름
    color=None #색상
    
    #생성자
    def __init__(self, company, name, color):
        print("__init() 생성자 호출됨")
        self.company=company
        self.name=name
        self.color=color


if __name__ == '__main__':
    pass