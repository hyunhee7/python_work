#-*- coding: utf-8 -*-

#tuple 타입 = args / #dict 타입 = kwargs
# 어떤 인자의 형태도 받아줄 수 있는 함수
def test(*args, **kwargs):
    print args
    print kwargs
    print "--- test() ---"


if __name__ == '__main__':
    test(1)
    test(1,2)
    test("a","b","c")
    test(name="gura")
    test(isMan=True)
    test(name="monkey",isMan=False)
    test("a","b",name="cat",isMan=True)
