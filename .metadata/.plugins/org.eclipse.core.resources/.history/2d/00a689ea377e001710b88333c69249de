#-*- coding: utf-8 -*-
import re
import os
import codecs

sample = u"a b c ddd    eee"
result=re.split(r"[\t]+",sample)

print result

cwd=os.getcwd()
filePath=cwd+os.sep
filePath=cwd+os.sep+"testFile.txt"
f=codecs.open(filePath,"r","utf-8")

while True:
    data=f.readline()
    if data=="":
        break #반복문 블럭 빠져 나오기
    print data